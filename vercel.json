{
  "version": 2,
  "builds": [
    {
      "src": "freakfit/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "freakfit/wsgi.py"
    }
  ],
  "hooks": {
    "build": "python manage.py collectstatic --noinput"
  }
}
